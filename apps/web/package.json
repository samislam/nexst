{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "prettier": "@samislam/prettier-config/next",
  "scripts": {
    "dev": "dotenv -e .env.development next dev --turbopack",
    "build": "dotenv -e .env.production next build",
    "start": "dotenv -e .env.production next start",
    "format": "prettier --write \"./**/*.{htm,html,css,md,mdx,js,jsx,ts,tsx}\"",
    "lint": "next lint",
    "clean": "del \"**/{node_modules,.next,.turbo}\"",
    "tolgee": "dotenv -e .env.development tolgee",
    "locales:sync": "bun tolgee sync",
    "locales:print": "bun tolgee extract print",
    "locales:compare": "bun tolgee compare",
    "locales:pull": "bun tolgee pull",
    "locales:push": "bun tolgee push"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@tolgee/react": "^5.31.5",
    "await-to-js": "^3.0.0",
    "axios": "^1.7.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.460.0",
    "next": "15.0.3",
    "next-themes": "^0.4.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-hook-form": "^7.53.2",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tailwindest": "^2.3.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@samislam/prettier-config": "^1.1.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "del-cli": "^6.0.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
