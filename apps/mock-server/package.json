{
  "name": "mock-server.nexst",
  "type": "module",
  "prettier": "@samislam/prettier-config",
  "scripts": {
    "dev": "concurrently --names \"merge,json-server\" \"tsx --watch ./server/merge.ts\" \"json-server ./server/db.json --port 5500 \"",
